<div class="modal hide" id="select_excel_format_modal">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
    <h3><%= t(:label_select_xls_version) %></h3>
  </div>
  <div class="modal-body">
    <input type="hidden" id="cur_template_id"/>
    <input type="radio" name="excel_format" id="format_xls"/>
    <%= "Excel 2003(.xls)" %>
    &nbsp;&nbsp;&nbsp;&nbsp;
    <input type="radio" name="excel_format" id="format_xlsx" checked="checked">
    <%= "Excel 2007(Above)(.xlsx)" %>
  </div>
  <div class="modal-footer">
    <button type="submit" class="btn btn-primary" onclick="export_data(event)"><%= t(:export) %></button>
    <button type="submit" class="btn" data-dismiss="modal"><%= t(:close) %></button>
  </div>
</div>