<div class="modal hide" id="modal_interface_wnd">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
  </div>
  <div class="modal-body">
    <div id="modal_interface_wnd_msg"></div>
    <form id="modal_interface_wnd_form">
    <table class="detailList" cellpadding="0" cellspacing="0" border="0">
      <tbody>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_no) %></label></td>
        <td class="data-col"><input type="text" id="_interface_no" style="width: 300px"/></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_code) %></label></td>
        <td class="data-col"><input type="text" id="_interface_code" style="width: 300px"/></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_name) %></label></td>
        <td class="data-col"><input type="text" id="_interface_name" style="width: 300px"/></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_version) %></label></td>
        <td class="data-col"><input type="text" id="_interface_version" style="width: 300px"/></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_context) %></label></td>
        <td class="data-col"><input type="text" id="_interface_context" style="width: 300px"/></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_desc) %></label></td>
        <td class="data-col"><input type="text" id="_interface_desc" style="width: 300px"/></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_server) %></label></td>
        <td class="data-col"><%= select("_interface","server",get_odi_server_list,{},:style=>"width: 310px") %></td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_category) %></label></td>
        <td class="data-col"><%= select("_interface","category",get_category(Dip::DipConstant::CATEGORY_ODI),{:include_blank=>true},:style=>"width: 310px") %></td>
      </tr>
      </tbody>
    </table>
    </form>
  </div>
  <div class="modal-footer">
    <button type="submit" class="btn btn-primary" id="save_header" onclick="save_change(event)"><%= t(:save) %></button>
    <button type="submit" class="btn" data-dismiss="modal"><%= t(:close) %></button>
  </div>
</div>
