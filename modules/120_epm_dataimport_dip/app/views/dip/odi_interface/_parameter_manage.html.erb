<div class="modal hide" id="parameter_wnd">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
  </div>
  <div class="modal-body">
    <div id="parameter_wnd_msg"></div>
    <table class="detailList" cellpadding="0" cellspacing="0" border="0">
      <tbody>
      <tr>
        <td class="label-col"><label><%= t(:label_dip_odi_interface_category) %></label></td>
        <td class="data-col">
          <input type="text" id="_category_name" style="width: 300px" readonly="readonly"/>
        </td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:label_parameter_set) %></label></td>
        <td class="data-col">
          <%= select("_","parameter_set_id",
                     Dip::ParameterSet.where({:param_type=>Dip::DipConstant::PARAMETER_ODI}).collect{|c|[c.name,c.id]},
                     {:include_blank=>true},:style=>"width: 310px") %>
        </td>
      </tr>
      </tbody>
    </table>
  </div>
  <div class="modal-footer">
    <%= link_to t(:label_parameter_set_manage), {:action => "odi_index", :controller => "dip/parameter_set"}, :class => "btn btn-primary" %>
    <button type="submit" class="btn btn-primary" id="save_header" onclick="save_parameter(event)"><%= t(:save) %></button>
    <button type="submit" class="btn" data-dismiss="modal"><%= t(:close) %></button>
  </div>
</div>
