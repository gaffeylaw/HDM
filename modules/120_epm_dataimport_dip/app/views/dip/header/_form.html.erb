<div class="modal hide" id="modal_header_wnd">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
  </div>
  <div class="modal-body">
    <div id="header_form_msg"></div>
    <table class="detailList" cellpadding="0" cellspacing="0" border="0">
      <tbody>
      <tr>
        <td class="label-col"><label><%= t(:code) %></label></td>
        <td class="data-col">
          <input type="text" id="_header_code" style="width: 300px"/>
        </td>
      </tr>
      <tr>
        <td class="label-col"><label><%= t(:name) %></label></td>
        <td class="data-col">
          <input type="text" id="_header_name" style="width: 300px"/>
        </td>
      </tr>
      </tbody>
    </table>
  </div>
  <div class="modal-footer">
    <button type="submit" class="btn btn-primary" id="save_header" onclick="save_header(event)"><%= t(:save) %></button>
    <button type="submit" class="btn" data-dismiss="modal"><%= t(:close) %></button>
  </div>
</div>
