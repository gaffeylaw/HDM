<div class="modal hide" id="modal_interface_wnd">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">Ã—</button>
  </div>
  <div class="modal-body">
    <div id="modal_interface_wnd_msg"></div>
    <form id='modal_interface_wnd_form'>
      <table class="detailList" cellpadding="0" cellspacing="0" border="0">
        <tbody>
        <tr>
          <td class="label-col"><label><%= t(:label_infa_workflow_name) %></label></td>
          <td class="data-col"><input type="text" id="_workflow_name" style="width: 300px"/></td>
        </tr>
        <tr>
          <td class="label-col"><label><%= t(:label_infa_workflow_name_alias) %></label></td>
          <td class="data-col"><input type="text" id="_workflow_name_alias" style="width: 300px"/></td>
        </tr>
        <tr>
          <td class="label-col"><label><%= t(:label_infa_workflow_folder_name) %></label></td>
          <td class="data-col"><input type="text" id="_workflow_folder_name" style="width: 300px"/></td>
        </tr>
        <tr>
          <td class="label-col"><label><%= t(:label_infa_workflow_repository) %></label></td>
          <td class="data-col"><%= select("_workflow","repository",get_repository,{},:style=>"width: 310px") %></td>
        </tr>
        <tr>
          <td class="label-col"><label><%= t(:label_infa_workflow_category) %></label></td>
          <td class="data-col"><%= select("_workflow","category",get_category(Dip::DipConstant::CATEGORY_INFA),{:include_blank=>true},:style=>"width: 310px") %></td>
        </tr>
        </tbody>
      </table>
    </form>
  </div>
  <div class="modal-footer">
    <button type="submit" class="btn btn-primary" onclick="save_change(event)"><%= t(:save) %></button>
    <button type="submit" class="btn" data-dismiss="modal"><%= t(:close) %></button>
  </div>
</div>