<%=datatable_view({:columns=>[:name,:code,:description],:count=>@count,:datas=>@datas}) do |builder|%>
  <% builder.column(:id)%>
  <% builder.column(:name,{:title=>t(:label_irm_group_name), :searchable => true, :width => "250px"})%>
  <% builder.column(:code,{:title=>t(:label_irm_group_group_code), :width => "60px"})%>
  <% builder.column(:description,{:title=>t(:label_irm_group_description)})%>
<% end %>