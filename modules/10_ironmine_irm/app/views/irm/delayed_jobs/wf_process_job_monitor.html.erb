<%= app_title %>
<div class="listRelatedObject setupBlock">
    <div class="page-block">
      <div class="pbHeader">
        <table cellpadding="0" cellspacing="0" border="0">
          <tbody>
            <tr>
              <td class="pbTitle"><h2 class="mainTitle"><%= t(:label_irm_delayed_jobs_record) %></h2></td>
              <td id="topButtonRow" class="pbButton">

              </td>
              <td class="pbHelp">
                <div id="permissionsSearchBox"></div>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="pbBody">
        <div id="delayed_job" class="datatable" style="width:100%;">
          <div id="delayed_job_id" style="display:none">
            <%= link_to(t(:show),{:action=>"item_list",:delayed_job_id=>"{delayed_job_id}"}) %>
           </div>
        </div>
      </div>
        <%= datatable("delayed_job",{:action=>"get_data"},
                  [{:key=>"delayed_job_id",:label=>t(:edit),:formatter=>"Y.irm.template",:width=>"40px"},
                   {:key=>"job_status",:label=>t(:label_irm_delayed_jobs_record_status),:width=>"10%"},
                   {:key=>"priority",:label=>t(:label_irm_delayed_jobs_record_priority),:width=>"10%"},
                   {:key=>"handler",:label=>t(:label_irm_delayed_jobs_record_handler),:width=>"15%"},
                   {:key=>"run_at",:label=>t(:label_irm_delayed_jobs_record_run_at),:searchable=>true,:width=>"40px"},
                   {:key=>"end_at",:label=>t(:label_irm_delayed_jobs_record_end_at),:width=>"40px"}],
                   {:search_box=>"permissionsSearchBox",:paginator_box=>"companiesPaginatorBox"})%>
        <div id="companiesPaginatorBox"></div>
      </div>
    </div>
</div>