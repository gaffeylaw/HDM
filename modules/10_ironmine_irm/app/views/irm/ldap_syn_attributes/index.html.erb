    <td class="page-block-help">
      <div id="ldap_syn_attributesSearchBox"></div>
    </td>
  </tr></table>
  <div class="page-block-body">
    <%= datatable("ldap_syn_attributes",{:action=>"get_data"},[],{:paginator_box=>"ldap_syn_attributesPaginatorBox",:search_box=>"ldap_syn_attributesSearchBox"})%>
  </div>
</div>


