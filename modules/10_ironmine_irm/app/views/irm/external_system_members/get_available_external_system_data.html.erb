<%=datatable_view({:columns=>[:external_system_code,:system_name,:system_description],:count=>@count,:datas=>@datas}) do |builder|%>
  <% builder.column(:id)%>
  <% builder.column(:system_name,{:title=>t(:label_irm_menu_name), :searchable => true, :width => "250px"})%>
  <% builder.column(:external_system_code,{:title=>t(:label_irm_external_system), :width => "60px"})%>
  <% builder.column(:system_description,{:title=>t(:description)})%>
<% end %>

