<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
  <% builder.column(:id,{:title=>t(:operation), :width => "80px"}) do |data|%>
    <%= link_to(t(:edit),{:action=>"edit",:id=>data[:id]}) %>&nbsp;
    <%= link_to(t(:show),{:action=>"show",:id=>data[:id]}) %>
  <% end %>
  <% builder.column(:name,{:title=>t(:name),:searchable=>true, :width => "140px"})  %>
  <% builder.column(:description,{:title=>t(:description), :width => "180px"})%>
  <% builder.column(:grades,{:title=>t(:label_irm_rating_config_grade)}) do |data|%>
    <%= display_rating_config_grade(data) %>
  <% end %>
<% end %>