<body style="padding:5px;">
  <!--页面结构-->
  <div class="page-block">
    <table class="page-block-header"><tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= Irm::BusinessObject.class_name_to_meaning(@business_object.bo_model_name) %></h2>
      </td>
      <td class="page-block-button"></td>
      <td class="page-block-help"></td>
    </tr></table>
    <div class="page-block-body">
      <% if @datas.any? %>
          <% if lookup_context.template_exists?("#{@business_object.bo_model_name.pluralize.underscore}/lov",[],true) %>
              <%= render :partial => "#{@business_object.bo_model_name.pluralize.underscore}/lov" %>
          <% else %>
              <%= render :partial => "lov_result" %>
          <% end %>
      <% else %>
        <%=   render :partial => "helper/datatable_no_data" %>
      <% end %>
    </div>
  </div>
</body>