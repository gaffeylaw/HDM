<%= page_title %>

<!--页面结构-->
<div class="page-block">
  <table class="page-block-header">
    <tr>
      <td class="page-block-title">
        <h2 class="block-title"><%= t(:label_irm_profile_list) %></h2>
      </td>
      <td class="page-block-button">
        <%= link_to t(:new), {:action => "new"},:class=>"btn"%>
      </td>
      <td class="page-block-help">
        <div id="profilesSearchBox"></div>
      </td>
    </tr>
  </table>
  <div class="page-block-body">
    <div id="profiles"></div>
    <%= datatable("profiles",{:action=>"get_data"},[],{:paginator_box=>"profilesPaginatorBox",:search_box=>"profilesSearchBox"})%>
    <div id="profilesPaginatorBox"></div>
  </div>
</div>


