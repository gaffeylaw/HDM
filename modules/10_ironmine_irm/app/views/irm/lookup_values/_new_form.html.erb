
<%= setting_title %>
<%= form_for(@lookup_value,:url=>{:action=>"create"},:builder => CustomFormBuilder) do |f| %>
  <%=f.hidden_field :lookup_type%>

  <% content_for :form_block_a   do%>
        <table class="form-table">
                <tbody>
                    <tr>
                      <td class="label-col"><label><%=(t :label_lookup_code)%></label></td>
                      <td class="data-col"><%=f.text_field :lookup_code,:required=>true,:irm_uppercase=>true %></td>
                    </tr>
                    <tr>
                      <td class="label-col"><label><%=(t :label_meaning)%></label></td>
                      <td class="data-col"><%=f.text_field :meaning,:required=>true%></td>
                    </tr>
                    <tr>
                      <td class="label-col"><label for="date1"><%=(t :label_start_date_active)%></label></td>
                      <td class="data-col">
                        <%=f.date_field(:start_date_active,:required=>true)%>
                      </td>
                    </tr>
                    <tr>
                      <td class="label-col"><label for="date2"><%=(t :label_end_date_active)%></label></td>
                      <td class="data-col">
                        <%=f.date_field(:end_date_active) %>
                      </td>
                    </tr>
                    <tr>
                      <td class="label-col"><label><%=(t :label_description)%></label></td>
                      <td class="data-col"><%=f.text_area :description,:rows=>4%></td>

                    </tr>
                </tbody>
              </table>
  <% end %>

  <% content_for :form_buttons  do %>
    <%= link_submit t(:save),{:class=>"btn btn-primary"}%>
    <%= link_back t(:cancel),{},{:class=>"btn cancel"}%>
  <% end %>



    <!--页面结构-->
    <div class="page-block">
      <table class="page-block-header"><tr>
        <td class="page-block-title">
          <h2 class="block-title"><%=t(:label_lookup_value_new)%></h2>
        </td>
        <td class="page-block-button">
          <%= content_for :form_buttons   %>

        </td>
      </tr></table>
      <div class="page-block-body">
        <%= error_for @lookup_value%>
        <div class="sub-page-block">
          <div class="sub-page-block-header">
            <%= form_require_info %>
            <h3 class="sub-page-block-title"><%= t(:label_irm_lookup_value_info) %></h3>
          </div>
          <div class="sub-page-block-body">
            <%= content_for :form_block_a   %>
          </div>
        </div>

      </div>

      <table class="page-block-footer"><tr>
        <td class="page-block-title">
        </td>
        <td class="page-block-button">
          <%= content_for :form_buttons   %>
        </td>
      </tr></table>
    </div>

<% end %>
