<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
    <% builder.column(:id,{:title=>t(:operation), :width => "80px"}) do |data|%>
          <%= link_to(t(:edit),{:action=>"edit",:id=>data[:id]},{:back=>{:action=>"index"}}) %>
          <%= link_to(t(:show),{:action=>"show",:id=>data[:id]},{:back=>{:action=>"index"}}) %>
    <% end %>
    <% builder.column(:name,{:title=>t(:label_irm_wf_field_update_name)})%>
    <% builder.column(:field_update_code,{:title=>t(:label_irm_wf_field_update_field_update_code),:searchable=>true})%>
    <% builder.column(:bo_name,{:title=>t(:label_irm_wf_field_update_bo_code),:searchable=>true})%>
    <% builder.column(:object_attribute_name,{:title=>t(:label_irm_wf_field_update_object_attribute)})%>
    <% builder.column(:value_type_name,{:title=>t(:label_irm_wf_field_update_value_type)})%>
<% end %>