<%=datatable_view({:count=>processes.count,:datas=>processes}) do |builder|%>
    <% builder.column(:id,{:title=>t(:operation), :width => "120px"}) do |data|%>
        <%= link_to(t(:edit),{:action=>"edit",:id=>data.id},:back=>true) %>
        <%= link_to(t(:active),{:action=>"active",:id=>data.id,:active=>Irm::Constant::SYS_YES},:back=>true) %>
        <%= link_to(t(:delete),{:action=>"destroy",:id=>data.id,:back_url=>url_for({})},{:back=>true,:confirm => t(:label_are_you_sure) ,:method => :delete} )%>
    <% end %>

    <% builder.column(:name,{:title=>t(:label_irm_wf_approval_process_name)}) do |data|%>
        <%= link_to(data.name,{:action=>"show",:id=>data.id},:back=>true) %>
    <% end %>
    <% builder.column(:description,{:title=>t(:label_irm_wf_approval_process_description)})%>
<% end %>