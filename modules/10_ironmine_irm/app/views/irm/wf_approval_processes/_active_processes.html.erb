<%=datatable_view({:count=>processes.count,:datas=>processes}) do |builder|%>
    <% builder.column(:id,{:title=>t(:operation), :width => "80px"}) do |data|%>
        <%= link_to(t(:disable),{:action=>"active",:id=>data.id,:active=>Irm::Constant::SYS_NO,:back_url=>url_for({})}) %>
    <% end %>
    <% builder.column(:process_order,{:title=>t(:label_irm_wf_approval_process_process_order)}) do |data|%>
        <%= text_field_tag("process_orders[#{data.id}]",data.process_order,:size=>5) %>
    <% end %>
    <% builder.column(:name,{:title=>t(:label_irm_wf_approval_process_name)}) do |data|%>
        <%= link_to(data.name,{:action=>"show",:id=>data.id},:back_url=>url_for({})) %>
    <% end %>
    <% builder.column(:description,{:title=>t(:label_irm_wf_approval_process_description)})%>
<% end %>