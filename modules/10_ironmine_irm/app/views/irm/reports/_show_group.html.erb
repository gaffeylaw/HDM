<style type="text/css">
  table#reportData td,th{
       padding: 3px 2px 3px 5px;
  }
  tr.group{

  }
  tr.group1{
    background-color: #D5DDF3;
  }
  tr.group2{
    background-color: #EFF4FC;
  }
  table#reportData tr.group2 td{
     padding: 3px 2px 3px 30px;
  }
  tr.break1{
    background-color: #D5DDF3;
  }
  tr.break2{
    background-color: #EFF4FC;
  }
</style>
<% if auto_run?(@report) %>
    <% if @report.show_chart? %>
        <%= render "irm/report_chart/#{@report.chart_type.downcase}"%>
    <% end %>
    <table id="reportData" cellspacing="0" cellpadding="0" border="0" style="table-layout: fixed; width:100%">
      <%= raw group_report(@report) %>
    </table>
<% end %>