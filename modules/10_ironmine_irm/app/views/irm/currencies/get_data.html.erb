<%=datatable_view({:count=>@count,:datas=>@datas}) do |builder|%>
  <% builder.column(:id,{:title=>t(:operation),:width=>"80px"}) do |data| %>
    <%= link_to(t(:edit),{:action=>"edit",:id=>data[:id]}) %>&nbsp;
    <%= link_to(t(:show),{:action=>"show",:id=>data[:id]}) %>
  <% end %>
  <% builder.column(:currency_code,{:title=>t(:label_irm_currency_currency_code),:searchable=>true})%>
  <% builder.column(:precision,{:title=>t(:label_irm_currency_precision)})%>
  <% builder.column(:name,{:title=>t(:label_irm_currency_name),:searchable=>true})%>
  <% builder.column(:description,{:title=>t(:label_irm_currency_description)})%>
  <% builder.column(:status_meaning,{:title=>t(:label_irm_language_status_code),:width=>"40px"}) %>
<% end %>