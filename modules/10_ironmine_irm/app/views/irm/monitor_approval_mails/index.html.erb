<%= app_title %>
<!--页面结构-->
<div class="page-block">
  <table class="page-block-header"><tr>
    <td class="page-block-title">
      <h2 class="block-title"><%= t(:label_irm_delayed_job_wf_approval_mail_monitor) %></h2>
    </td>
  </tr></table>
  <div class="page-block-body">
    <%=datatable_view({:count=>@monitor.count,:datas=>@monitor}) do |builder|%>
      <% builder.column(:approval_process_name,{:title=>t(:label_irm_wf_approval_process_name_full)})%>
      <% builder.column(:bo_name,{:title=>t(:label_irm_business_object_name_full)})%>
      <% builder.column(:bo_description,{:title=>t(:label_irm_business_object_description_full)})%>
      <% builder.column(:approval_step_name,{:title=>t(:label_irm_wf_approval_step_name_full)})%>
      <% builder.column(:run_at,{:title=>t(:label_irm_delayed_jobs_record_run_at)}) do |log|%>
        <%=log[:run_at].strftime("%F %T") if log[:run_at]%>
      <% end %>
      <% builder.column(:end_at,{:title=>t(:label_irm_delayed_jobs_record_end_at)}) do |log|%>
        <%=log[:end_at].strftime("%F %T") if log[:end_at]%>
      <% end %>
      <% builder.column(:job_status,{:title=>t(:label_irm_delayed_jobs_record_status)})%>
    <% end %>
  </div>
</div>