<div class="sub-block">
    <h3 class="lbHeader"></h3>
    <table cellpadding="0" cellspacing="0" border="0" style="width:100%">
      <tbody>
        <tr>
          <td class="pbTitle"><h3 class="lbHeader"><%= t(:label_irm_wf_my_process_step_instance) %></h3></td>
          <td id="topButtonRow" class="pbButton">
          </td>
          <td class="pbHelp">
            <%= blank_select(:task_entry,my_task_entries) %>
          </td>
        </tr>
      </tbody>
    </table>
    <div class="pbBody">
    <div class="pbSubsection">
        <div class="datatable">
          <div class="irm-datatable">
            <div>
                <table>
                  <thead>
                    <tr>
                      <th>
                        <div>
                          <%= t(:type) %>
                        </div>
                      </th>
                      <th>
                        <div>
                          <%= t(:label_irm_task_title) %>
                        </div>
                      </th>
                      <th>
                        <div>
                          <%= t(:label_irm_task_status_name) %>
                        </div>
                      </th>
                      <th>
                        <div>
                          <%= t(:label_irm_task_start_at) %>
                        </div>
                      </th>
                      <th>
                        <div>
                          <%= t(:label_irm_task_end_at) %>
                        </div>
                      </th>
                    </tr>
                  </thead>

                   <tbody>
                       <% tasks.each do |task|%>
                         <tr id="task_<%=task[:id]%>">
                           <td>
                             <div>
                               <%= Irm::BusinessObject.class_name_to_meaning(task[:type])%>
                              </div>
                           </td>
                           <td>
                             <div>
                               <%= link_to task[:title],task[:url_options] %>
                              </div>
                           </td>
                           <td>
                             <div>
                               <%= task[:status_name] %>
                              </div>
                           </td>
                           <td>
                             <div>
                               <%= task[:start_at] %>
                             </div>
                           </td>
                           <td>
                             <div>
                               <%= task[:end_at] %>
                             </div>
                           </td>
                         </tr>
                       <% end %>
                   </tbody>

                </table>
            </div>
          </div>
        </div>
    </div>
    </div>
</div>